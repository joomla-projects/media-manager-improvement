<?xml version="1.0" encoding="utf-8"?>
<form addfieldprefix="Joomla\Component\Content\Administrator\Field">
	<fieldset>
		<field
			name="extension_id"
			type="text"
			label="JGLOBAL_FIELD_ID_LABEL"
			default="0"
			readonly="true"
			class="readonly" 
		/>

		<field
			name="name"
			type="hidden"
			label="COM_PLUGINS_FIELD_NAME_LABEL"
		/>

		<field
			name="enabled"
			type="list"
			label="JSTATUS"
			class="custom-select-color-state"
			size="1"
			default="1"
			>
			<option value="1">JENABLED</option>
			<option value="0">JDISABLED</option>
		</field>

		<field
			name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			size="1" 
		/>

		<field
			name="ordering"
			type="Pluginordering"
			label="JFIELD_ORDERING_LABEL"
		/>

		<field
			name="folder"
			type="text"
			label="COM_PLUGINS_FIELD_FOLDER_LABEL"
			class="readonly"
			size="20"
			readonly="true" 
		/>

		<field
			name="element"
			type="text"
			label="COM_PLUGINS_FIELD_ELEMENT_LABEL"
			class="readonly"
			size="20"
			readonly="true" 
		/>
	</fieldset>
</form>
